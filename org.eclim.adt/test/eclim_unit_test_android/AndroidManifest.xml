<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.eclim.adt.test"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="21" />

    <!-- 
      triggering completion here should provide "application",
      "permission", etc.
      -->

    <!-- 
      `android:` inside here should trigger many attributes,
        and `="` should trigger resource completion
      -->
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".EclimAdtTestActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <!-- action names should be provided after `="` -->
                <action android:name="android.intent.action.MAIN" />

                <!-- as above, but categories -->
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
